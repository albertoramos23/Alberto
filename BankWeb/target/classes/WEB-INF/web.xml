<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>MostrarCuentaServlet</servlet-name>
  	<display-name>MostrarCuentaServlet</display-name>
  	<description></description>
  	<servlet-class>com.curso.banco.web.MostrarCuentaServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PruebaServlet</servlet-name>
  	<display-name>PruebaServlet</display-name>
  	<description></description>
  	<servlet-class>com.curso.banco.web.PruebaServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ListaClientesServlet</servlet-name>
  	<display-name>ListaClientesServlet</display-name>
  	<description></description>
  	<servlet-class>com.curso.banco.web.ListaClientesServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>MostrarCuentaServlet</servlet-name>
  	<url-pattern>/mostrarCuentaServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PruebaServlet</servlet-name>
  	<url-pattern>/PruebaServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListaClientesServlet</servlet-name>
  	<url-pattern>/ListaClientesServlet</url-pattern>
  </servlet-mapping>
</web-app>
